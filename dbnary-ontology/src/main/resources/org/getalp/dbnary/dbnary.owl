    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://kaiko.getalp.org/dbnary#exampleSource -->

    <owl:DatatypeProperty rdf:about="&dbnary;exampleSource"/>
    


    <!-- http://kaiko.getalp.org/dbnary#gloss -->

    <owl:DatatypeProperty rdf:about="&dbnary;gloss">
        <rdfs:comment xml:lang="en">A textual gloss that may be used to disambiguate the translation&apos;s source sense. </rdfs:comment>
        <rdfs:domain rdf:resource="&dbnary;Translation"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://kaiko.getalp.org/dbnary#note -->

    <owl:DatatypeProperty rdf:about="&dbnary;note">
        <rdfs:comment xml:lang="en">Textual note that may be used to add information to a LemonElement when no standard vocabularies exists to explicit it.</rdfs:comment>
        <rdfs:domain rdf:resource="&lemon;LemonElement"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://kaiko.getalp.org/dbnary#partOfSpeech -->

    <owl:DatatypeProperty rdf:about="&dbnary;partOfSpeech">
        <rdfs:comment xml:lang="en">The original part of speech as it was defined in the Wiktionary page. This property takes a String litteral as object.</rdfs:comment>
        <rdfs:domain rdf:resource="&lemon;LexicalEntry"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://kaiko.getalp.org/dbnary#senseNumber -->

    <owl:DatatypeProperty rdf:about="&dbnary;senseNumber">
        <rdfs:comment xml:lang="en">The number of the lexical Sense in the Lexical Entry, as given by Wiktionary. This property takes a String literal as an object.</rdfs:comment>
        <rdfs:domain rdf:resource="&lemon;LexicalSense"/>
    </owl:DatatypeProperty>
    


    <!-- http://kaiko.getalp.org/dbnary#targetLanguageCode -->

    <owl:DatatypeProperty rdf:about="&dbnary;targetLanguageCode">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The target language of a given translation. This may be used when the dbnary:targetLanguage object property can not be used.</rdfs:comment>
        <rdfs:domain rdf:resource="&dbnary;Translation"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://kaiko.getalp.org/dbnary#usage -->

    <owl:DatatypeProperty rdf:about="&dbnary;usage">
        <rdfs:comment xml:lang="en">Usage note that is used to further annotate the target of the translation.</rdfs:comment>
        <rdfs:domain rdf:resource="&dbnary;Translation"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://kaiko.getalp.org/dbnary#writtenForm -->

    <owl:DatatypeProperty rdf:about="&dbnary;writtenForm">
        <rdfs:comment xml:lang="en">The written form of a translation equivalent.</rdfs:comment>
        <rdfs:domain rdf:resource="&dbnary;Translation"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


